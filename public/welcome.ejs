<h2>Welcome</h2>
<p>Welcome to your Budgetting Portal</p>

<noscript>
	You must have Javascript Enabled to use this application.
</noscript>

<section name="accounts">
	<!--
		Load Accounts Here. They'll come in the form of
		Name,Balance,Date Updated
	-->
	<h3>Your Accounts</h3>
	<table name="accounts-list">
		<thead>
			<tr>
				<th></th>
				<th>ID</th>
				<th>Name</th>
				<th>Balance</th>
			</tr>
		</thead>
		<tbody>			
		</tbody>
	</table>
</section>

<section name="account-display">
	<h3>---</h3>
	<table reversed name="lineitems">
		<thead>
			<tr>
				<th>Date</th>
				<th>Name</th>
				<th>Amount</th>
				<th>Latitude</th>
				<th>Longitude</th>
			</tr>
		</thead>
		<tbody>
		</tbody>
	</table>
</section>
