GET /assets/*file controllers.Assets.at(path="/public", file)

GET /auth bgi.controllers.Dashboard.index
POST /register bgi.controllers.Auth.register
POST /login bgi.controllers.Auth.login

GET / bgi.controllers.Dashboard.dashboard
GET /categories bgi.controllers.Category.categories
POST /categories/create bgi.controllers.Category.create

GET /test bgi.controllers.Dashboard.test
